[init]
	defaultBranch = main
[include]
	path = config.local
[core]
	editor = vim
	quotepath = false
[push]
	default = simple
[pull]
	rebase = false
	ff = only
[diff]
	algorithm = histogram
[color]
	ui = auto
[pager]
	log = diff-highlight | less
	show = diff-highlight | less
	diff = diff-highlight | less
[alias]
	conflicts = diff --name-only --diff-filter=U
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
